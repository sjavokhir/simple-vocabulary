<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cardDetail"
    app:startDestination="@id/cardDetailFragment">

    <fragment
        android:id="@+id/cardDetailFragment"
        android:name="uz.javokhirdev.svocabulary.presentation.carddetail.CardDetailFragment"
        android:label="@string/cards"
        tools:layout="@layout/fragment_card_detail">
        <argument
            android:name="cardId"
            android:defaultValue="-1L"
            app:argType="long" />
        <argument
            android:name="setId"
            android:defaultValue="-1L"
            app:argType="long" />
    </fragment>

</navigation>